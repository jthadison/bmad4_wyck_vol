{
  "dashboard": {
    "title": "Signal Scanner Performance",
    "uid": "signal-scanner-perf",
    "tags": ["wyckoff", "signals", "performance"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Signal Generation Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(signals_generated_total[5m])",
            "legendFormat": "{{pattern_type}} - {{symbol}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "Signals/min", "format": "short"},
          {"format": "short", "show": false}
        ],
        "xaxis": {"mode": "time", "show": true},
        "legend": {"show": true, "values": false, "current": false}
      },
      {
        "id": 2,
        "title": "Detection Latency Percentiles",
        "type": "graph",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(pattern_detection_latency_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(pattern_detection_latency_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(pattern_detection_latency_seconds_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"label": "Latency (s)", "format": "s"},
          {"format": "short", "show": false}
        ],
        "xaxis": {"mode": "time", "show": true},
        "legend": {"show": true, "values": true, "current": true},
        "alert": {
          "name": "High Pattern Detection Latency",
          "conditions": [
            {
              "evaluator": {"type": "gt", "params": [0.5]},
              "operator": {"type": "and"},
              "query": {"params": ["B", "5m", "now"]},
              "reducer": {"type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "message": "Pattern detection p95 latency > 500ms",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 3,
        "title": "Active Symbols",
        "type": "stat",
        "gridPos": {"x": 0, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "active_symbols_count",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 50, "color": "yellow"},
                {"value": 100, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Scanner Health",
        "type": "stat",
        "gridPos": {"x": 6, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "scanner_health",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "background"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "type": "value",
                "options": {
                  "1": {"text": "Healthy", "color": "green"},
                  "0": {"text": "Unhealthy", "color": "red"}
                }
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Pending Signals",
        "type": "stat",
        "gridPos": {"x": 12, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "pending_signals_count",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 25, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Approval vs Rejection Rate",
        "type": "piechart",
        "gridPos": {"x": 18, "y": 8, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(signals_approved_total[5m]))",
            "legendFormat": "Approved",
            "refId": "A"
          },
          {
            "expr": "sum(rate(signals_rejected_total[5m]))",
            "legendFormat": "Rejected",
            "refId": "B"
          }
        ],
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"},
          "pieType": "pie",
          "displayLabels": ["percent"]
        }
      },
      {
        "id": 7,
        "title": "Signal Validation Latency",
        "type": "graph",
        "gridPos": {"x": 0, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(signal_validation_latency_seconds_bucket[5m]))",
            "legendFormat": "{{pattern_type}} p95",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "Latency (s)", "format": "s"},
          {"format": "short", "show": false}
        ],
        "xaxis": {"mode": "time", "show": true},
        "legend": {"show": true, "values": true, "current": true}
      },
      {
        "id": 8,
        "title": "WebSocket Notification Latency",
        "type": "graph",
        "gridPos": {"x": 12, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(websocket_notification_latency_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, rate(websocket_notification_latency_seconds_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"label": "Latency (s)", "format": "s"},
          {"format": "short", "show": false}
        ],
        "xaxis": {"mode": "time", "show": true},
        "legend": {"show": true, "values": true, "current": true}
      },
      {
        "id": 9,
        "title": "Rejection Reasons Breakdown",
        "type": "graph",
        "gridPos": {"x": 0, "y": 20, "w": 24, "h": 8},
        "targets": [
          {
            "expr": "rate(signals_rejected_total[5m])",
            "legendFormat": "{{rejection_stage}} - {{pattern_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "Rejections/min", "format": "short"},
          {"format": "short", "show": false}
        ],
        "xaxis": {"mode": "time", "show": true},
        "legend": {"show": true, "values": false, "current": false},
        "stack": true
      },
      {
        "id": 10,
        "title": "API Error Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 28, "w": 24, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(api_requests_total{status_code=~\"4..\"}[5m])) by (endpoint)",
            "legendFormat": "4xx - {{endpoint}}",
            "refId": "A"
          },
          {
            "expr": "sum(rate(api_requests_total{status_code=~\"5..\"}[5m])) by (endpoint)",
            "legendFormat": "5xx - {{endpoint}}",
            "refId": "B"
          },
          {
            "expr": "sum(rate(api_requests_total[5m]))",
            "legendFormat": "Total requests",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"label": "Requests/sec", "format": "reqps"},
          {"format": "short", "show": false}
        ],
        "xaxis": {"mode": "time", "show": true},
        "legend": {"show": true, "values": true, "current": true, "avg": true},
        "seriesOverrides": [
          {
            "alias": "/^5xx.*/",
            "color": "red",
            "linewidth": 2
          },
          {
            "alias": "/^4xx.*/",
            "color": "orange"
          },
          {
            "alias": "Total requests",
            "color": "blue",
            "dashes": true,
            "fill": 0
          }
        ],
        "alert": {
          "name": "High API Error Rate",
          "conditions": [
            {
              "evaluator": {"type": "gt", "params": [0.1]},
              "operator": {"type": "and"},
              "query": {"params": ["B", "5m", "now"]},
              "reducer": {"type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "message": "API 5xx error rate > 0.1 req/s",
          "noDataState": "ok",
          "notifications": []
        }
      },
      {
        "id": 11,
        "title": "Entry Type Distribution (Story 13.10)",
        "type": "piechart",
        "gridPos": {"x": 0, "y": 36, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "sum(entries_by_type_total) by (entry_type)",
            "legendFormat": "{{entry_type}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"},
          "pieType": "pie",
          "displayLabels": ["name", "percent"]
        }
      },
      {
        "id": 12,
        "title": "Entry Type Win Rate (Story 13.10)",
        "type": "gauge",
        "gridPos": {"x": 8, "y": 36, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "entry_type_win_rate",
            "legendFormat": "{{entry_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.5, "color": "yellow"},
                {"value": 0.6, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 13,
        "title": "BMAD Workflow Stage Completions (Story 13.10)",
        "type": "graph",
        "gridPos": {"x": 16, "y": 36, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "rate(bmad_stage_completions_total[5m])",
            "legendFormat": "{{stage}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "Completions/min", "format": "short"},
          {"format": "short", "show": false}
        ],
        "xaxis": {"mode": "time", "show": true},
        "legend": {"show": true, "values": true, "current": true},
        "stack": true
      },
      {
        "id": 14,
        "title": "Entry Type Evaluation Latency (Story 13.10)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 44, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(entry_type_evaluation_latency_seconds_bucket[5m]))",
            "legendFormat": "{{entry_type}} p95",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "Latency (s)", "format": "s"},
          {"format": "short", "show": false}
        ],
        "xaxis": {"mode": "time", "show": true},
        "legend": {"show": true, "values": true, "current": true}
      },
      {
        "id": 15,
        "title": "Entry Priority Decisions (Story 13.10)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 44, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(entry_priority_decisions_total[5m])",
            "legendFormat": "Chose {{chosen_type}} over {{competing_types}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "Decisions/min", "format": "short"},
          {"format": "short", "show": false}
        ],
        "xaxis": {"mode": "time", "show": true},
        "legend": {"show": true, "values": false, "current": false}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    },
    "annotations": {
      "list": []
    },
    "templating": {
      "list": []
    }
  }
}
