{
  "name": "bmad-wyckoff-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:check": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:smoke": "playwright test",
    "test:smoke:headed": "playwright test --headed",
    "test:smoke:report": "playwright show-report",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@types/dompurify": "^3.0.5",
    "axios": "^1.13.2",
    "big.js": "^7.0.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.1",
    "lightweight-charts": "^4.1.0",
    "marked": "^17.0.1",
    "pinia": "^2.1.0",
    "primeicons": "^7.0.0",
    "primevue": "^3.50.0",
    "uuid": "^13.0.0",
    "vue": "^3.4.0",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.7",
    "@playwright/test": "^1.57.0",
    "@types/big.js": "^6.2.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/ui": "^1.6.1",
    "@vue/test-utils": "^2.4.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-vue": "^9.20.0",
    "happy-dom": "^20.0.11",
    "postcss": "^8.4.0",
    "prettier": "^3.2.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.9.3",
    "vite": "^5.0.0",
    "vitest": "^1.2.0",
    "vue-tsc": "^3.1.6"
  },
  "optionalDependencies": {
    "@rollup/rollup-win32-x64-msvc": "^4.54.0"
  }
}
