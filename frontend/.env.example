# Environment Configuration Template
# Copy this file to .env.production and configure for your environment

# REST API Base URL
# Development: http://localhost:8000/api/v1
# Production: https://your-domain.com/api/v1
VITE_API_BASE_URL=http://localhost:8000/api/v1

# WebSocket Base URL
# Development: ws://localhost:8000/ws
# Production: wss://your-domain.com/ws
VITE_WS_BASE_URL=ws://localhost:8000/ws

# Application Version (updated automatically by CI/CD)
VITE_APP_VERSION=0.1.0

# Sentry DSN for error tracking (optional)
# Get this from your Sentry project settings
# VITE_SENTRY_DSN=https://your-key@o123456.ingest.sentry.io/123456
